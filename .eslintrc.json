{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "standard",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "rules": {
        "semi": 0,
        "space-before-function-paren": [
            "error",
            "never"
        ],
        "no-return-assign": "off"
    }
}